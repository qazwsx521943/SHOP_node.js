<%- include('./includes/head.ejs') %>
<%- include('./includes/navbar.ejs') %>
<main>
    <div class="container">
        <table class="table">
            <thead>
              <tr>
                <th scope="col">id</th>
                <th scope="col">email</th>
                <th scope="col">password</th>
                <th scope="col">name</th>
              </tr>
            </thead>
            <tbody>
            <% users.forEach(user => { %>
              <tr>
                <th scope="row"><%=user.user_id %></th>
                <td><%=user.email %></td>
                <td><%=user.password %></td>
                <td><%=user.name %></td>
                <td><a href="/admin/user/<%= user.user_id %>"><i class="fa-sharp fa-solid fa-circle-info"></i></a></td>
              </tr>
            <% }) %>
            </tbody>
          </table>
    </div>
</main>
<%- include('./includes/end.ejs') %>